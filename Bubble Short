//bubble short
#include<stdio.h>
int main()
{
int arr[50],n,i,j;
printf("Enter the Size of the Array\n");
scanf("%d",&n);
printf("Enter the Elemnts in the Array\n");
for(i=0;i<n;i++)
{
scanf("%d",&arr[i]);
}
for(i=0;i<n;i++)
{
for(j=i+1;j<n;j++)
{
if(arr[i]>arr[j])
{
int temp=arr[i];
arr[i]=arr[j];
arr[j]=temp;
}
}
}
printf("the Elemnts in the Array:");
for(i=0;i<n;i++)
{
printf("%d",arr[i]);
}
return 0;
}





